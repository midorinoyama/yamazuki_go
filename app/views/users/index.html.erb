<h2>ユーザー一覧</h2>
<div class="container">
  <div class="row">
    <% @users.each do |user| %>

    <div class="col-md-3">
      <div class="">
        <%= link_to user_path(user.id) do %>
          <%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg", class:"mt-3 rounded-circle"; %>
        <% end %>
        <%= user.nickname %>

      </div>
    </div>
    <% end %>
  </div>
</div>