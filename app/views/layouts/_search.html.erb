<div class="form-inline my-2 my-lg-0">
  <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.select :model, options_for_select({ "山名" => "post", "ユーザー" => "user", }) %>
    <%= f.text_field :content, class: "" %>
    <% f.select :method, options_for_select({ "部分一致" => "partial" }) %>
    <%= f.submit "検索", class: "btn btn-outline-success" %>
  <% end %>
</div>